{
  "name": "artwrite",
  "version": "1.0.0",
  "homepage": "http://artwriteproductions.com",
  "dependencies": {
    "@google-cloud/debug-agent": "^2.6.0",
    "babel-plugin-dynamic-import-node": "^2.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "file-loader": "^1.1.6",
    "ignore-styles": "^5.0.1",
    "jest-cli": "^23.6.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.4",
    "react-social-icons": "^3.0.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "url-loader": "^1.0.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar ./src/css/styles.css -o ./build/styles.css",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/css/styles.css -o ./build/styles.css --watch --recursive",
    "start": "npm-run-all -p serve watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "start-js": "react-scripts start",
    "serve": "NODE_ENV=production BABEL_ENV=production node --inspect server/bootstrap.js node --inspect server/bootstrap.js",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
